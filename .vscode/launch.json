  "version": "0.2.0",
  "configurations": [
    {
      "name": "App Dev",
      "request": "launch",
      "type": "dart",
      "program": "lib/main_dev.dart",
      "args": [
        // flavor => dev
        "--flavor",
        "dev",

        "t",
        "/lib/main_dev.dart"
      ]
    },
    {
      "name": "App Staging",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main_staging.dart",
      "args": [
        // flavor => staging
        "--flavor",
        "staging",
        "t",
        "/lib/main_staging.dart"
      ]
    },
    {
      "name": "App Prod",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main_prod.dart",
      "args": [
        // flavor => prod
        "--flavor",
        "prod",

        "t",
        "/lib/main_prod.dart"
      ]
    }
  ]
}
